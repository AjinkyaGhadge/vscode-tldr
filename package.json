{
  "name": "vscode-tldr",
  "displayName": "vscode-tldr",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Snippets",
    "Other"
  ],
  "keywords": [
    "tldr",
    "man",
    "shell",
    "cli",
    "command",
    "bash",
    "howto"
  ],
  "activationEvents": [
    "onLanguage:dockerfile",
    "onLanguage:makefile",
    "onLanguage:powershell",
    "onLanguage:shellscript",
    "onLanguage:bat"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "tl;dr",
      "properties": {
        "tldr.platform": {
          "type": "string",
          "items": {
            "type": "object",
            "title": "Platforms",
            "properties": {
              "name": {
                "type": "Auto-Defect",
                "description": "X"
              },
              "size": {
                "type": "Windows",
                "description": "X"
              }
            }
          },
          "default": false,
          "description": "Complete functions with their parameter signature."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
